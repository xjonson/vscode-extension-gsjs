{
  "name": "gsjs",
  "displayName": "GSJS",
  "description": "A VSCode extension for GS",
  "keywords": ["gs", "gsjs"],
	"version": "0.0.7",
	"publisher": "jonson",
	"icon": "images/icon.png",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
		"onLanguage:javascript",
    "onCommand:extension.helloWorld"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
    },
    "commands": [{
      "command": "extension.helloWorld",
      "title": "Hello World"
    }],
    "snippets": [{
      "language": "javascript",
      "path": "./snippets/js-common.json"
    },{
      "language": "javascript",
      "path": "./snippets/js-card.json"
    },{
      "language": "javascript",
      "path": "./snippets/js-list.json"
    },{
      "language": "javascript",
      "path": "./snippets/js-gsp.json"
    },{
      "language": "javascript",
      "path": "./snippets/js-zj.json"
    },{
      "language": "javascript",
      "path": "./snippets/js-control.json"
    }]
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "eslint": "^6.6.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
	"license": "SEE LICENSE IN LICENSE.txt",
	"bugs": {
			"url": "https://github.com/xjonson/vscode-extension-gsjs/issues"
	},
	"repository": {
			"type": "git",
			"url": "https://github.com/xjonson/vscode-extension-gsjs"
	},
	"homepage": "https://github.com/xjonson/vscode-extension-gsjs/"
}