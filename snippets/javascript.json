			"\t\tvar AOPPreMsg = JSON.parse(result.AOPPreMsg)",
			"\t\tif(AOPPreMsg.${1:ZJJSDJ}) {",
			"\t\t\t$.each(AOPPreMsg.${1:ZJJSDJ}, function(i, val) {",
			"\t\t\t\t$.notify.info(val.CLJG);",
			"\t\t\t})",
			"\t\t} else {",
			"\t\t\t$.notify.info(result.AOPPreMsg);",
			"\t\t}",
			"\t\treturn false;",
			"\t} ",
			"\telse {$4}",
			"})",
		]
	},
